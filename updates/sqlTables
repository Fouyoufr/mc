parties=>pUri=>varchar(8) NOT NULL,pMechant=>int(8) NOT NULL DEFAULT 0,pMechVie=>int(3) NOT NULL DEFAULT 0,pMechDesoriente=>tinyint(1) NOT NULL DEFAULT 0,pMechSonne=>tinyint(1) NOT NULL DEFAULT 0,pMechTenace=>tinyint(1) NOT NULL DEFAULT 0,pMechRiposte=>tinyint(1) NOT NULL DEFAULT 0,pMechPercant=>tinyint(1) NOT NULL DEFAULT 0,pMechDistance=>tinyint(1) NOT NULL DEFAULT 0,pMechPhase=>enum('1';'2';'3') NOT NULL DEFAULT '1',pDate=>datetime NOT NULL DEFAULT current_timestamp(),pPremier=>int(8) NOT NULL,pManiMax=>int(3) NOT NULL DEFAULT 0,pManiAcceleration=>int(1) NOT NULL DEFAULT 0,pManiCourant=>int(3) NOT NULL DEFAULT 0,pManiPrincipale=>int(8) NOT NULL DEFAULT 0,PRIMARY KEY=>pUri
