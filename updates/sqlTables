joueurs=>jId=>int(8) NOT NULL AUTO_INCREMENT,jPartie=>varchar(8) NOT NULL,jNom=>varchar(50) NOT NULL,jVie=>int(3) NOT NULL DEFAULT 0,jStatut=>enum('AE';'SH') NOT NULL DEFAULT 'AE',jDesoriente=>tinyint(1) NOT NULL DEFAULT 0,jSonne=>tinyint(1) NOT NULL DEFAULT 0,jTenace=>tinyint(1) NOT NULL DEFAULT 0,jHeros=>int(8) NOT NULL,jNumero=>enum('1';'2';'3';'4') NOT NULL,jOnline=>datetime NOT NULL DEFAULT current_timestamp(),PRIMARY KEY=>jId
